<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- 圆形ImageView -->
    <declare-styleable name="CircleImageView">
        <attr name="borderW" format="dimension" />
        <attr name="borderColor" format="color" />
    </declare-styleable>

    <!-- 虚线 -->
    <declare-styleable name="DashLine">
        <attr name="strokeWidth2" format="dimension" />
        <attr name="strokeColor2" format="reference|color" />
    </declare-styleable>

    <!-- 自定义RationBar -->
    <declare-styleable name="RationBar">
        <attr name="stepSize" format="float" />
        <attr name="rating" format="float" />
        <attr name="targetW" format="dimension" />
        <attr name="targetH" format="dimension" />
        <attr name="starPadding" format="dimension" />
        <attr name="numStars" format="integer" />
        <attr name="normalStar" format="reference" />
        <attr name="selStar" format="reference" />
    </declare-styleable>

    <declare-styleable name="ZLoadingView">
        <attr name="z_type">
            <enum name="CircleBuilder" value="0" />
            <enum name="ClockBuilder" value="1" />
            <enum name="StarBuilder" value="2" />
            <enum name="LeafBuilder" value="3" />
            <enum name="DoubleCircleBuilder" value="4" />
            <enum name="PacManBuilder" value="5" />
            <enum name="ElasticBallBuilder" value="6" />
            <enum name="InfectionBallBuilder" value="7" />
            <enum name="IntertwineBuilder" value="8" />
            <enum name="TextBuilder" value="9" />
            <enum name="SearchPathBuilder" value="10" />
            <enum name="RotateCircleBuilder" value="11" />
            <enum name="SingleCircleBuilder" value="12" />
            <enum name="SnakeCircleBuilder" value="13" />
        </attr>
        <attr name="z_color" format="color" />
    </declare-styleable>

</resources>